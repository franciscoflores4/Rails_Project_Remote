<h1>Search Results:</h1>
<h1>Jerseys</h1>
<% @js.each do |jersey| %>
	<p><%= link_to image_tag(jersey.image), jersey_path(jersey) %></p>
	<div class ="player">
		<h3><%= jersey.player[:name] %></h3>
		<h3><%= jersey.team[:name] %></h3>
		<h3><%= jersey.size %></h3>
		<h2><%= jersey.price %></h2>
	</div>
<% end %>
<%= render partial: 'jersey', object: @js %>
<p><%= link_to 'Back', home_index_path%></p>
